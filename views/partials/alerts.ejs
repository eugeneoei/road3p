
<% if (alerts.error) { %>
  <% alerts.error.forEach(function(msg) { %>
    <div id='alert' class="alert alert-danger"><%= msg %></div>
  <% }); %>
<% } %>
<% if (alerts.success) { %>
  <% alerts.success.forEach(function(msg) { %>
    <div id='alert' class="alert alert-success"><%= msg %></div>
  <% }); %>
<% } %>
